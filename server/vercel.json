{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/migrate",
      "destination": "api/migrate.js"
    },
    {
      "source": "/api/seed",
      "destination": "api/seed.js"
    },
    {
      "source": "/(.*)",
      "destination": "api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}