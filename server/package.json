{
  "name": "stylay-api",
  "version": "1.0.0",
  "description": "Stylay E-commerce API - Multi-vendor e-commerce platform",
  "main": "app.js",
  "scripts": {
    "start": "node --watch app.js",
    "dev": "nodemon app.js",
    "tunnel": "node src/scripts/tunnel.js",
    "test": "jest --watchAll --verbose --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:redis-fix": "REDIS_ENABLED=false jest --verbose --coverage",
    "lint": "eslint . --ext .js",
    "format": "prettier --write \"**/*.js\"",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit --fix",
    "security:check": "npm audit && npm outdated",
    "start:secure": "NODE_ENV=production node app.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "status": "npx sequelize-cli db:migrate:status",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:file": "npx sequelize-cli db:seed --seed",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "setup": "npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:dev": "pm2 start ecosystem.config.js --env development",
    "pm2:stop": "pm2 stop stylay-server",
    "pm2:delete": "pm2 delete stylay-server",
    "pm2:restart": "pm2 restart stylay-server",
    "pm2:logs": "pm2 logs stylay-server",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "pm2:save": "pm2 save",
    "pm2:startup": "pm2 startup"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["./jest.setup.js"],
    "testMatch": [
      "**/src/tests/**/*.test.js",
      "**/src/test/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/tests/**",
      "!src/test/**",
      "!jest.setup.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"],
    "verbose": true,
    "forceExit": true,
    "detectOpenHandles": true
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "clamscan": "^2.4.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^6.2.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "libphonenumber-js": "^1.12.29",
    "morgan": "^1.10.0",
    "mysql2": "^3.15.3",
    "nodemailer": "^7.0.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "pm2": "^6.0.14",
    "prom-client": "^15.1.3",
    "redis": "^5.9.0",
    "sequelize": "^6.31.0",
    "slugify": "^1.6.6",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@ngrok/ngrok": "^1.6.1",
    "chalk": "^4.1.2",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "nodemon": "^3.1.11",
    "prettier": "^2.8.3",
    "sequelize-cli": "^6.5.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "ecommerce",
    "api",
    "nodejs",
    "express",
    "sequelize",
    "mysql"
  ],
  "author": "Stylay Team",
  "license": "MIT"
}
